{
  "builds": [
    {
      "src": "Port-folio/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/Images/(.*)", "destination": "/Images/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
