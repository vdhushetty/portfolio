{
  "buildCommand": "cd Port-folio && npm run build",
  "outputDirectory": "Port-folio/dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/Images/(.*)", "destination": "/Images/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
